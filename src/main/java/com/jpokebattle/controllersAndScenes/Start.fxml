<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.com.jpokebattle.controllersAndScenes.StartController">
   <children>
      <ImageView fx:id="background" fitHeight="480.0" fitWidth="640.0" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="120.0" layoutY="35.0" text="Pokemonne (an original game)" textFill="WHITE">
         <font>
            <Font name="Algerian" size="25.0" />
         </font>
      </Label>
      <Label fx:id="createdBy" alignment="CENTER_RIGHT" layoutX="437.0" layoutY="460.0" onMouseClicked="#handleCreatedBy" onMouseEntered="#handleCreatedByMouseEntered" onMouseExited="#handleCreatedByMouseExited" prefHeight="14.0" prefWidth="199.0" text="surely created by me " textAlignment="RIGHT" textFill="WHITE">
         <font>
            <Font name="Algerian" size="12.0" />
         </font>
      </Label>
      <Button fx:id="startButton" layoutX="230.0" layoutY="372.0" mnemonicParsing="false" onAction="#handleStart" onMouseEntered="#handleStartMouseEntered" onMouseExited="#handleStartMouseExited" prefHeight="37.0" prefWidth="180.0" text="-&gt; new game &lt;-" textAlignment="CENTER">
         <font>
            <Font name="Algerian" size="20.0" />
         </font>
      </Button>
      <ProgressBar fx:id="progressBar" layoutX="220.0" layoutY="382.0" prefWidth="200.0" progress="0.0" />
      <Label fx:id="progressLabel" layoutX="306.0" layoutY="410.0" text="0%" />
   </children>
</AnchorPane>
